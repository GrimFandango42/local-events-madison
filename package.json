{
  "name": "local-events",
  "version": "1.0.0",
  "description": "Madison event discovery platform - Facebook-free event aggregation",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "lib": "src",
    "test": "tests"
  },
  "scripts": {
    "dev": "next dev -p 5000 -H 0.0.0.0",
    "dev:oneclick": "node scripts/one-click-dev.js",
    "build": "next build",
    "build:prod": "NODE_ENV=production next build -f next.config.production.js",
    "build:analyze": "ANALYZE=true npm run build:prod",
    "build:staging": "NODE_ENV=production next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start -p 3000",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "npm run db:generate && npm run db:push && jest",
    "test:unit": "vitest run --config tests/vitest.config.ts",
    "test:unit:watch": "vitest --config tests/vitest.config.ts",
    "test:unit:coverage": "vitest run --coverage --config tests/vitest.config.ts",
    "test:unit:ui": "vitest --ui --config tests/vitest.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:performance": "playwright test --project='Performance Tests'",
    "test:accessibility": "playwright test --project='Accessibility Tests' --project='Mobile Accessibility'",
    "test:api": "vitest run tests/api --config tests/vitest.config.ts",
    "test:components": "vitest run tests/components --config tests/vitest.config.ts",
    "test:all": "npm run test:unit:coverage && npm run test:e2e && npm run test:performance && npm run test:accessibility",
    "test:parallel": "node scripts/test-runner.js",
    "test:parallel:quick": "node scripts/test-runner.js --skip-performance --skip-accessibility",
    "test:parallel:no-e2e": "node scripts/test-runner.js --skip-e2e",
    "test:parallel:sequential": "node scripts/test-runner.js --no-parallel",
    "test:ci": "node scripts/test-runner.js --bail",
    "test:quick": "npm run test:unit && npm run test:e2e --project='Desktop Chrome'",
    "test:scraper": "node scripts/test-scrapers.js",
    "start:collector": "tsx backend/services/MCPEventCollector.ts",
    "scrape:once": "tsx backend/services/run-once.ts",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "node scripts/create-sample-data.js",
    "db:indexes": "node scripts/apply-indexes.js",
    "db:performance": "node scripts/db-performance.js",
    "db:performance:monitor": "node scripts/db-performance.js --mode=monitor",
    "db:performance:optimize": "node scripts/db-performance.js --mode=optimize",
    "setup": "node scripts/quick-setup.js",
    "setup:city": "node scripts/quick-setup.js",
    "config:validate": "node scripts/validate-config.js",
    "links:check": "tsx scripts/check-links.ts",
    "purge:old": "tsx scripts/purge-old-events.ts",
    "dev:clean": "node scripts/dev-clean.js",
    "analyze": "node scripts/analyze-bundle.js",
    "perf:test": "node scripts/performance-test.js",
    "perf:monitor": "npm run perf:test && npm run analyze",
    "lint:perf": "eslint . --ext .ts,.tsx --max-warnings 0",
    "security:audit": "npm audit --audit-level high",
    "security:scan": "tsx scripts/security-scan.ts",
    "validate:env": "tsx scripts/validate-env.ts",
    "cache:warm": "node scripts/cache-warm.js",
    "cache:monitor": "node scripts/cache-monitor.js monitor",
    "cache:status": "node scripts/cache-monitor.js status",
    "cache:test": "node scripts/cache-monitor.js test",
    "cache:health": "node scripts/cache-monitor.js health",
    "cache:invalidate": "node scripts/cache-invalidate.js",
    "cache:clear": "node scripts/cache-invalidate.js all",
    "deepseek:analyze": "node scripts/claude-deepseek-integration.js analyze",
    "deepseek:security": "node scripts/claude-deepseek-integration.js security",
    "deepseek:performance": "node scripts/claude-deepseek-integration.js performance",
    "docker:build": "docker build -t local-events .",
    "docker:run": "docker run -p 3000:3000 local-events",
    "docker:prod": "docker-compose -f docker-compose.yml up -d",
    "docker:staging": "docker-compose -f docker-compose.staging.yml up -d",
    "docker:down": "docker-compose down",
    "deploy:vercel": "vercel deploy --prod",
    "deploy:railway": "railway deploy",
    "db:migrate:prod": "DATABASE_URL=$DATABASE_URL npx prisma migrate deploy",
    "db:migrate:staging": "DATABASE_URL=$DATABASE_URL npx prisma migrate deploy",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "events",
    "madison",
    "scraping",
    "mcp",
    "local-events",
    "event-discovery",
    "wisconsin",
    "nextjs",
    "chatgpt-connector",
    "facebook-alternative",
    "privacy-focused",
    "venue-scraping",
    "event-aggregation"
  ],
  "author": "Local Events Platform",
  "license": "MIT",
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.0.0",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18.0.0",
    "@types/react-virtualized-auto-sizer": "^1.0.4",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^10.0.0",
    "critters": "^0.0.23",
    "dompurify": "^3.2.6",
    "googleapis": "^159.0.0",
    "isomorphic-dompurify": "^2.26.0",
    "lucide-react": "^0.294.0",
    "luxon": "^3.4.4",
    "next": "^14.0.0",
    "next-auth": "^4.24.11",
    "node-fetch": "^3.3.2",
    "playwright": "^1.46.0",
    "prisma": "^5.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^2.1.0",
    "react-window-infinite-loader": "^1.0.10",
    "redis": "^4.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0",
    "uuid": "^13.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@babel/core": "^7.25.0",
    "@babel/runtime": "^7.25.0",
    "@next/bundle-analyzer": "^14.0.0",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.0.0",
    "@types/luxon": "^3.4.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.21",
    "axe-playwright": "^2.2.0",
    "babel-jest": "^29.7.0",
    "c8": "^9.0.0",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-testing-library": "^6.0.0",
    "happy-dom": "^12.0.0",
    "jest": "^29.0.0",
    "jest-environment-node": "^29.7.0",
    "msw": "^2.0.0",
    "node-mocks-http": "^1.14.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "tsx": "^4.19.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "webpack-bundle-analyzer": "^4.9.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "optionalDependencies": {
    "@mendable/firecrawl-js": "^4.3.3",
    "@playwright/mcp": "^0.0.36"
  }
}
